{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/openroot-helping-hand/**",
        "destination": "https://comeonsom.github.io/openroot-helping-hand/",
        "type": 301
      },
      {
        "source": "/stock-averaging/**",
        "destination": "https://comeonsom.github.io/stock-averaging/",
        "type": 301
      },
      {
        "source": "/return-analyzer/**",
        "destination": "https://comeonsom.github.io/investment-analyzer/",
        "type": 301
      },
      {
        "source": "/gold-jewel-price-indicator/**",
        "destination": "https://comeonsom.github.io/Gold-Jewellery-Price-Calculator/",
        "type": 301
      },
      {
        "source": "/openroot-creditcard-emi-analyzer/**",
        "destination": " https://comeonsom.github.io/openroot-creditcard-emi-analyzer/",
        "type": 301
      },
      {
        "source": "/openroot-travel-expense-manager/**",
        "destination": " https://comeonsom.github.io/openroot-travel-expense-manager/",
        "type": 301
      },
      {
        "source": "/makaut-grade-calculator/**",
        "destination": "https://comeonsom.github.io/makaut_grade_converter/",
        "type": 301
      }
    ]
  }
}
